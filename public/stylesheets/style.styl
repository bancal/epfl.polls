grayscale()
  filter: url("data:image/svg+xml;utf8,<svg version='1.1' xmlns='http://www.w3.org/2000/svg' height='0'><filter id='greyscale'><feColorMatrix type='matrix' values='0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0' /></filter></svg>#greyscale");
  -webkit-filter unquote("grayscale(1.0)")

nograyscale()
  filter none
  -webkit-filter none

body
  margin-bottom: 60px;

a
  color: #00B7FF

form
  height: 300px;

form ul
  height:60px;
  list-style none

form li
  height:60px;
  border 1px solid red
  border-radius 10px
  margin-bottom 15px
  padding 10px 10px 10px 70px
  background-color white
  background-repeat no-repeat
  background-size 32px 32px
  background-position: 14px 14px;
  grayscale()
  cursor: move;

form label
  margin-right 10px
  background-color #ffffff
  cursor inherit

form li:hover
  nograyscale()

li#conf
  background-image: url("/images/Farm-Fresh_reseller_programm.png");

li#food
  background-image: url("/images/Farm-Fresh_chinese_noodle.png");

li#sport
  background-image: url("/images/Farm-Fresh_sport_basketball.png");

li#team
  background-image: url("/images/Farm-Fresh_gruppe5.png");

#content
  padding: 60px 15px 0;

.navbar-fixed-bottom
  // In addition to bootstrap's own style, make it
  // opaque:
  background-color: #f5f5f5;

.navbar-fixed-bottom > .container {
  padding-right: 15px;
  padding-left: 15px;
}
